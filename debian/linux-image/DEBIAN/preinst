#!/bin/sh

set -e

# Pass maintainer script parameters to hook scripts
export DEB_MAINT_PARAMS="$*"

# Tell initramfs builder whether it's wanted
export INITRD=Yes

test -d /etc/kernel/preinst.d && run-parts --arg="5.15.67-cmsc421-project0-jghorml1+" --arg="/boot/vmlinuz-5.15.67-cmsc421-project0-jghorml1+" /etc/kernel/preinst.d
exit 0
